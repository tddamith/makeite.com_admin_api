{
  "canvas": {
    "width": 700,
    "height": 900
  },
  "editable": {
    "text": [
      {
        "id": "inviteText",
        "selector": "[data-editable='inviteText']",
        "type": "text"
      },
      {
        "id": "brideName",
        "selector": "[data-editable='brideName']",
        "type": "text",
        "fontFamily": "Great Vibes",
        "fontSize": 55
      },
      {
        "id": "groomName",
        "selector": "[data-editable='groomName']",
        "type": "text",
        "fontFamily": "Great Vibes",
        "fontSize": 55
      },
      {
        "id": "saveTheDate",
        "selector": "[data-editable='saveTheDate']",
        "type": "text"
      },
      {
        "id": "eventDate",
        "selector": "[data-editable='eventDate']",
        "type": "text"
      },
      {
        "id": "eventTime",
        "selector": "[data-editable='eventTime']",
        "type": "text"
      },
      {
        "id": "location",
        "selector": "[data-editable='location']",
        "type": "text"
      }
    ],
    "images": [
      {
        "id": "flowerBorder",
        "selector": ".flower-border",
        "type": "image",
        "locked": true
      },
      {
        "id": "topFloral",
        "selector": ".top-floral",
        "type": "image"
      }
    ]
  }
}
