{
  "canvas": {
    "width": 520,
    "height": 720
  },
  "editable": {
    "text": [
      {
        "id": "intro",
        "selector": "[data-editable='intro']",
        "type": "text",
        "default": "TOGETHER WITH THEIR FAMILIES"
      },
      {
        "id": "names",
        "selector": "[data-editable='names']",
        "type": "text",
        "default": "Vicky & Damaris",
        "fontFamily": "Great Vibes",
        "fontSize": 48
      },
      {
        "id": "inviteText",
        "selector": "[data-editable='inviteText']",
        "type": "text",
        "default": "INVITE YOU TO CELEBRATE THEIR MARRIAGE"
      },
      {
        "id": "dayTime",
        "selector": "[data-editable='dayTime']",
        "type": "text",
        "default": "SUNDAY 01:00 PM"
      },
      {
        "id": "date",
        "selector": "[data-editable='date']",
        "type": "text",
        "default": "18",
        "fontSize": 40
      },
      {
        "id": "monthYear",
        "selector": "[data-editable='monthYear']",
        "type": "text",
        "default": "OCTOBER 2025"
      },
      {
        "id": "footerText",
        "selector": "[data-editable='footerText']",
        "type": "text",
        "default": "INVITE YOU TO CELEBRATE THEIR MARRIAGE"
      }
    ],
    "images": [
      {
        "id": "topFlowers",
        "selector": ".top-flowers img",
        "type": "image"
      }
    ]
  }
}
