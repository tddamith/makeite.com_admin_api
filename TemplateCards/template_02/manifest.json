{
  "canvas": {
    "width": 760,
    "height": 900
  },
  "editable": {
    "text": [
      {
        "id": "shortNames",
        "selector": "[data-editable='shortNames']",
        "type": "text"
      },
      {
        "id": "mainNames",
        "selector": "[data-editable='mainNames']",
        "type": "text",
        "fontFamily": "Great Vibes",
        "fontSize": 56
      },
      {
        "id": "surname",
        "selector": "[data-editable='surname']",
        "type": "text"
      },
      {
        "id": "eventTitle",
        "selector": "[data-editable='eventTitle']",
        "type": "text",
        "fontFamily": "Playfair Display",
        "fontSize": 44
      },
      {
        "id": "dateNumber",
        "selector": "[data-editable='dateNumber']",
        "type": "text",
        "fontSize": 88
      },
      {
        "id": "dateLine1",
        "selector": "[data-editable='dateLine1']",
        "type": "text"
      },
      {
        "id": "dateLine2",
        "selector": "[data-editable='dateLine2']",
        "type": "text"
      },
      {
        "id": "location",
        "selector": "[data-editable='location']",
        "type": "text"
      },
      {
        "id": "description",
        "selector": "[data-editable='description']",
        "type": "text"
      },
      {
        "id": "rsvp",
        "selector": "[data-editable='rsvp']",
        "type": "text"
      },
      {
        "id": "footerNote",
        "selector": "[data-editable='footerNote']",
        "type": "text"
      }
    ],
    "images": [
      {
        "id": "flowerBorder",
        "selector": ".flower-border",
        "type": "image",
        "locked": true
      },
      {
        "id": "textureImage",
        "selector": ".texture",
        "type": "image"
      }
    ]
  }
}
